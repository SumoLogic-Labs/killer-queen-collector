<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
  <Appenders>
    <Console name="STDOUT" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss,SSS Z} [%t] %-5p %c - %m%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="all" additivity="false">
      <AppenderRef ref="STDOUT" level="warn" />
    </Root>
  </Loggers>
</Configuration>
